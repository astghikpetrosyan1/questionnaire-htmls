<div class="alogaritmh-html">
    <style type="text/css">
        .title-subcase {
            margin-top: 10px;
        }

        .page_refero__component_group > div {
            padding-bottom: 5px;
            border-bottom: 1px solid #E1E1E1;
        }

        .output-list li {
            padding: 0px !important;
            font-size: 14px !important;
        }

        .grouped-containers {
            display: flex;
            gap: 24px;
            flex-direction: column;
        }

        .container {
            margin-bottom: unset !important;
            min-height: 200px;
            display: flex;
        }

        .container-text {
            display: flex;
            flex: 1;
        }

        .formSection {
            background: rgb(255, 255, 255) !important;
        }

        @media only screen and (max-width: 900px) {
            .grouped-containers {
                grid-template-columns: repeat(2, minmax(0, 1fr));
            }
        }

        @media only screen and (max-width: 670px) {
            .grouped-containers {
                grid-template-columns: repeat(1, minmax(0, 1fr));
            }
        }
    </style>
    <div class="header-title-con"
         style="text-align: justify; width: 100%; background-color: #fff; font-size: 20px; font-weight: 600; overflow: hidden; color: #000000; margin: 15px 0; box-shadow: 1px 2px 9px rgba(0,0,0,0.3); border-radius: 10px; padding: 100px 30px 30px 30px">
        <div style="color: #1399EC;width: 100%; margin-bottom: 20px;">
            <h2 style="font-weight: 600">
                Result checkcausesobesity.com
            </h2>
        </div>
        <div style="font-size: 19px;width:100%; font-weight: 400; color: #101011; line-height: normal">
            This questionnaire is made for people who are overweight or have severe overweight (obesity). The goal of
            completing the questionnaire is to gain more understanding of the possible causes of your overweight. You
            can share this result with your doctor or healthcare provider. This can help your healthcare provider to
            advise and treat you better, and to possibly perform extra tests.
        </div>
    </div>

    <div>
        <div style="font-size: 30px; color: #0070a8; font-weight: 500; margin: 20px 0; text-align: left">
            Summary
        </div>
        <div>
            <div>
                {{#if questionnaireResponse.item.[654].answer.[0].valueString}}

                    <!--     Is Bmi low               -->
                    {{#if (compare (toNumber questionnaireResponse.item.[654].answer.[0].valueString) 'lt' 18.5)}}
                        <div style="display: block; text-align: left; font-size: 18px; margin-bottom: 10px">
                            Your BMI is underweight. This questionnaire does not apply to you.
                        </div>
                    {{/if}}

                    <!--     Is Bmi medium               -->
                    {{#if (compare (toNumber questionnaireResponse.item.[654].answer.[0].valueString) 'ge' 18.5)}}
                        {{#if (compare (toNumber questionnaireResponse.item.[654].answer.[0].valueString) 'le' 24.9)}}
                            <div style="display: block; text-align: left; font-size: 18px">
                                There is a normal weight <span style="font-weight: bold; margin-left: 0px; margin-right: 0px">(BMI {{questionnaireResponse.item.[654].answer.[0].valueString}} kg/m<sup>2</sup>)</span>.
                                The following points of attention are known to increase weight:
                            </div>
                        {{/if}}
                    {{/if}}

                    <!--     Is Bmi high               -->
                    {{#if (compare (toNumber questionnaireResponse.item.[654].answer.[0].valueString) 'ge' 25)}}
                        {{#if (compare (toNumber questionnaireResponse.item.[654].answer.[0].valueString) 'le' 30)}}
                            <div style="display: block; text-align: left; font-size: 18px">
                                Based on your <b style="font-weight: bold; margin-left: 5px; margin-right: 5px">BMI {{questionnaireResponse.item.[654].answer.[0].valueString}} kg/m<sup>2</sup>)</b>, you are overweight with the following possible underlying causes
                            </div>
                        {{/if}}
                    {{/if}}

                    <!--     Is Bmi obese               -->
                    {{#if (compare (toNumber questionnaireResponse.item.[654].answer.[0].valueString) 'ge' 30)}}
                        <div style="display: block; text-align: left; font-size: 18px">
                            Based on your <b style="font-weight: bold; margin-left: 5px; margin-right: 5px">BMI ({{questionnaireResponse.item.[654].answer.[0].valueString}} kg/m<sup>2</sup>)</b>, you have obesity  with the following possible underlying causes
                        </div>
                    {{/if}}
                {{/if}}
            </div>
        </div>

    </div>

    {{! render each case calculations }}
    <div class="grouped-containers">
        {{#with questionnaireResponse}}
            {{#each item}}
                {{#if linkId}}
                    {{#if (lookup this "text")}}
                        {{#if (lookup this "answer")}}
                            <div class="container">
                                <div style="background-color: #B4C6E7; display: flex; align-items: center; justify-content: space-between; padding: 10px 20px; border-radius: 15px 15px 0px 0px;">
                                    <span class="case-title" style="color: #101011;width: 100%;padding: 10px 20px 10px 20px;">GENERAL CHARACTERISTICS</span>
                                </div>
                                <div class="container-text">
                                    <div class="dark-blue-container" style="background-color: #F3F7FA;">
                                        {{#each item}}
                                            {{#if answer}}
                                                <div class="row">
                                                    <div class="title-subcase">
                                                        {{text}}
                                                    </div>
                                                </div>

                                                {{#if (lookup this "text")}}
                                                    {{#if (lookup answer.[0] "item")}}
                                                        {{#each answer.[0].item}}
                                                            {{#if text}}
                                                                {{#if (lookup this "valueString")}}
                                                                    <i class="patient_que">{{valueString}}</i>
                                                                {{/if}}
                                                            {{/if}}
                                                        {{/each}}
                                                    {{/if}}
                                                {{/if}}

                                                {{#if (lookup answer.[0] "item")}}
                                                    <div>
                                                        {{#each answer.[0].item}}
                                                            {{#if valueBoolean}}
                                                                <div class="row">
                                                                    <div class="title-subcase-child">
                                                                        <span class="checked-icon"></span>
                                                                        {{#if text}}
                                                                            {{#if (lookup answer.[0] "item")}}
                                                                                {{text}}:
                                                                                <ul class="output-list">
                                                                                    {{#each answer.[0].item}}
                                                                                        {{#if valueBoolean}}
                                                                                            <li>{{text}}</li>
                                                                                        {{/if}}
                                                                                    {{/each}}
                                                                                </ul>
                                                                            {{else}}
                                                                                {{text}}
                                                                            {{/if}}
                                                                        {{/if}}
                                                                    </div>
                                                                </div>
                                                            {{/if}}
                                                        {{/each}}
                                                    </div>
                                                {{/if}}
                                            {{/if}}
                                        {{/each}}
                                    </div>
                                </div>
                            </div>
                        {{/if}}
                    {{/if}}
                {{/if}}
            {{/each}}
        {{/with}}

        <div>
            <p style="text-align: left; font-size: 20px; color: #101011; width: 100%; padding: 10px 20px 10px 20px; font-weight: 700 !important; background-color: #B4C6E7;">
                POSSIBLE UNDERLYING CAUSES OF OVERWEIGHT
            </p>
        </div>

        {{! Lifestyle factors section }}
        <div class="container">
            <div style="display: flex; align-items: center; justify-content: space-between; background-color: #F3F7FA; padding: 10px 20px; border-radius: 15px 15px 0px 0px;">
                <span class="case-title" style="color: #101011;width: 100%;padding: 10px 20px 10px 20px;">Lifestyle factors</span>
                <img alt="food-icon" src="./icon/icon-restaurant.png" style="width: 40px; height: 40px; margin-right: 15px"/>
            </div>

            <div class="container-text">
                <div class="dark-blue-container" style="background-color: #F3F7FA;">
                    <div class="subTitle" style="margin-top: 0 !important; padding: 10px 15px">
                        <p>An unhealthy lifestyle can play a role in developing obesity. Lifestyle factors can also increase the risk of certain diseases. The following factors, which can negatively affect your health, are noted in your questionnaire:</p>
                    </div>

                    {{#with questionnaireResponse}}
                        {{#each item}}
                            {{#if linkId}}
                                {{#each answer.[0].item}}
                                    {{#each answer.[0].item}}
                                        {{#if text}}
                                            <div class="title-subcase-child">
                                                <p style="text-decoration: underline">{{text}}</p>
                                                {{#unless valueBoolean}}
                                                    <div class="row" style="margin-left: 20px">
                                                        <span>There are no special dietary requirements for you</span>
                                                    </div>
                                                {{/unless}}
                                                {{#if valueBoolean}}
                                                    {{#each answer.[0].item}}
                                                        {{#if valueBoolean}}
                                                            <div class="row">
                                                                <div class="title-subcase-child">
                                                                    <span class="checked-icon"></span>
                                                                    {{text}}
                                                                </div>
                                                            </div>
                                                        {{/if}}
                                                    {{/each}}
                                                {{/if}}
                                            </div>
                                        {{/if}}
                                    {{/each}}
                                {{/each}}
                            {{/if}}
                        {{/each}}
                    {{/with}}
                </div>
            </div>
        </div>

        {{! Hypothalamic obesity section }}
        <div class="container">
            <div style="display: flex; align-items: center; justify-content: space-between; background-color: #F3F7FA; padding: 10px 20px; border-radius: 15px 15px 0px 0px;">
                <span class="case-title" style="color: #101011;width: 100%;padding: 10px 20px 10px 20px;">Hypothalamic obesity</span>
                <img alt="food-icon" src="./icon/icon-head.png" style="width: 45px; height: 45px; margin-right: 15px"/>
            </div>

            <div class="container-text">
                <div class="dark-blue-container" style="background-color: #F3F7FA;">
                    <div class="subTitle" style="margin-top: 0 !important; padding: 10px 15px">
                        {{#if questionnaireResponse.item.[331].answer.[0].valueBoolean}}
                            <p>Hypothalamic obesity is a rare cause of obesity. It is caused by damage to brain areas which
                                are important for hunger and feeling full. Based on your answers, it might be useful to talk
                                to your doctor about whether you should have further tests to check for a hypothalamic cause
                                of obesity.</p>
                        {{else}}
                            <p>Based on your answers, there are no signs of a hypothalamic cause of obesity (a type of
                                obesity due to damage to an area in the brain).</p>
                        {{/if}}
                    </div>
                </div>
            </div>
        </div>

        {{! GENETIC OBESITY section }}
        <div class="container">
            <div style="display: flex; align-items: center; justify-content: space-between; background-color: #F3F7FA; padding: 10px 20px; border-radius: 15px 15px 0px 0px; font-size: 22px;">
                <span class="case-title" style="color: #101011;width: 100%;padding: 10px 20px 10px 20px;">GENETIC OBESITY</span>
                <img alt="food-icon" src="./icon/icon-cell.png" style="width: 40px; height: 40px; margin-right: 15px"/>
            </div>
            <div class="container-text">
                <div class="dark-blue-container" style="background-color: #F3F7FA;">
                    <div class="subTitle" style="margin-top: 0 !important; padding: 10px 15px">
                        {{#if questionnaireResponse.item.[338].answer.[0].valueBoolean}}
                            <p>Genetic obesity is a rare type of obesity. Based on your answers, it might be useful to
                                discuss with your doctor whether you should have further tests for a genetic form of
                                obesity. The following features may fit with genetic obesity:</p>
                            {{#each questionnaireResponse.item.[338].item}}
                                {{#if valueBoolean}}
                                    <div class="row">
                                        <div class="title-subcase-child">
                                            <span class="checked-icon"></span>
                                            {{text}}
                                        </div>
                                    </div>
                                {{/if}}
                            {{/each}}
                        {{else}}
                            <p>Based on your answers, there are no signs of a genetic type of obesity.</p>
                        {{/if}}
                    </div>
                </div>
            </div>
        </div>

        {{! POSSIBLE CONSEQUENCES section }}
        {{#if questionnaireResponse.item.[480].answer.[0].valueBoolean}}
            <div class="container">
                <div style="display: flex; align-items: center; justify-content: space-between; background-color: #F3F7FA; padding: 10px 20px; border-radius: 15px 15px 0px 0px;">
                    <span class="case-title" style="background-color: #F3F7FA; color: #101011;width: 100%;padding: 10px 20px 10px 20px; font-weight: 700 !important;">POSSIBLE CONSEQUENCES OF BEING OVERWEIGHT</span>
                </div>
                {{#each questionnaireResponse.item.[480].item}}
                    <div class="container-text">
                        <div class="dark-blue-container" style="background-color: #F3F7FA;">
                            <div class="subTitle" style="margin-top: 0 !important; padding: 10px 15px">
                                {{subTitle}}
                            </div>
                            {{#each answer.[0].item}}
                                {{#if valueBoolean}}
                                    <div class="row">
                                        {{#if text}}
                                            <div class="title-subcase-child">
                                                <p style="text-decoration: underline">{{text}}</p>
                                                <div class="row" style="margin-left: 20px">
                                                    <span>Based on the combination of your answers, your risk of {{text}} may be increased.</span>
                                                </div>
                                            </div>
                                        {{/if}}
                                    </div>
                                {{/if}}
                            {{/each}}
                        </div>
                    </div>
                {{/each}}
            </div>
        {{/if}}

        {{! DISCLAIMER section }}
        <div>
            <h2 style="font-size: 24px; color: #1399EC; font-weight: bold; text-align: left; margin: 40px 0 8px 20px;">
                Completed results
            </h2>

            <div style="font-size: 18px; color: #101011; font-weight: 500; margin: 10px 0 16px 20px; text-align: left;">
                Your <strong>participation</strong> in the survey is greatly appreciated. <strong>Thank you!</strong>
            </div>
        </div>
    </div>
</div>

{{#each questionnaireResponse.item}}
        {{answer.[0].valueNumber}} - {{answer.[0].valueString}} - {{answer.[0].valueBoolean}} - {{text}} - {{@key}} - {{@index}}
        <hr>
        {{/each}}